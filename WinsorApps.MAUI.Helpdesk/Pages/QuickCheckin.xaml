<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:WinsorApps.MAUI.Helpdesk.ViewModels.Cheqroom"
             x:DataType="vm:QuickCheckinViewModel"
             x:Class="WinsorApps.MAUI.Helpdesk.Pages.QuickCheckin"
             Title="QuickCheckin">
    <VerticalStackLayout>
        <Label 
            Text="Asset Tag"
            StyleClass="H3"/>
        <Entry 
            Text="{Binding AssetTag}"/>
        <Button 
            Text="Check In"
            Command="{Binding CheckInCommand}" />
    </VerticalStackLayout>
</ContentPage>