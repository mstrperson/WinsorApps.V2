<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WinsorApps.MAUI.WorkoutAdmin.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:WinsorApps.MAUI.WorkoutAdmin.Pages"
    xmlns:local="clr-namespace:WinsorApps.MAUI.WorkoutAdmin"
    Shell.FlyoutBehavior="Flyout"
    Title="Fitness Center Admin">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Open Workouts"
        ContentTemplate="{DataTemplate pages:OpenWorkouts}"
        Route="OpenWorkouts" />

</Shell>
